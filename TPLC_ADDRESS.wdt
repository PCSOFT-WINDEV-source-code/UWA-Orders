  procedures :
   -
     name : DisplayAddress
     procedure_id : 1760075544926187599
     type_code : 14
     code : |1-
      // Summary: Displays the address
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE DisplayAddress()
      
      
      // Local variables
      
      
      // Searches for the Address record corresponding to this customer AND to this type of address
      HReadSeekFirst(Address, KEY_CustomerNumType, [Customer.CustomerNum,gsAddressType])
      // If the record exists
      IF HFound(Address) THEN
      	
      	// Displays information about this address
      	EDT_Address 	= Address.Address 
      	EDT_Address2 	= Address.Address2
      	EDT_City 		= Address.City
      	EDT_Status 		= Address.Report
      	EDT_ZipCode 	= Address.ZipCode
      	COMBO_Country 		= ListSeek(COMBO_Country,Address.Country, True)
      
      ELSE
      	// Displays the default information
      	EDT_Address 	= ""
      	EDT_Address2 	= ""
      	EDT_City 		= ""
      	EDT_Status 		= ""
      	EDT_ZipCode 	= ""
      	COMBO_Country 		= "France"
      END
     type : 458752
   -
     name : SaveAddress
     procedure_id : 1760075544926253135
     type_code : 14
     code : |1-
      // Summary: Saves the address
      // Syntax:
      // New_Procedure ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE SaveAddress()
      
      // Local variables
      
      
      // Searches for the Address record corresponding to this customer AND to this type of address
      HReadSeekFirst(Address, KEY_CustomerNumType, [Customer.CustomerNum,gsAddressType])
      // If the record does not exist
      IF HFound(Address) = False THEN
      	
      	// Creates the record
      	HReset(Address)
      	Address.CustomerNum	= Customer.CustomerNum
      	Address.Type		= gsAddressType
      	HAdd(Address)
      	
      END
      
      // Modifies the information about the address
      Address.Address 	= EDT_Address
      Address.Address2 	= EDT_Address2
      Address.City 		= EDT_City
      Address.Report 		= EDT_Status
      Address.ZipCode	= EDT_ZipCode
      Address.Country		= COMBO_Country..DisplayedValue
      HModify(Address)
     type : 458752
