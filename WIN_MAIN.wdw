   -
     name : DisplayPlane
     procedure_id : 1760075514859639602
     type_code : 14
     code : |1-
      PROCEDURE DisplayPlane(nPlane is int, bWithHistory is boolean = True)
      
      	IF UWPMenuOption > 0 THEN
      		// Restores the initial color
      		LOOP_MDL_MENU[UWPMenuOption]..BrushColor = IMG_TPL_MENUBACKGROUND..BrushColor
      		
      		IF bWithHistory THEN
      			Add(History, UWPMenuOption)
      		ELSE
      			IF History..Occurrence > 0 THEN
      				Delete(History, History..Occurrence)
      			END
      		END
      	END
      	
      	// Changes the selection color
      	UWPMenuOption = nPlane
      	LOOP_MDL_MENU[UWPMenuOption]..BrushColor = 0x6E4715
      	
      	pCallback is PROCEDURE = DisplayPreviousPlane
      	
      	SWITCH UWPMenuOption
      		// 1 - Customers
      		CASE 1
      			ChangeSourceWindow(CIW_TPL_SUPPORT, IW_CUSTOMERS, (History..Occurrence), pCallback) 			
      		// 2 - Products
      		CASE 2
      			ChangeSourceWindow(CIW_TPL_SUPPORT, IW_PRODUCTS, (History..Occurrence), pCallback)
      		// 3 - Orders
      		CASE 3
      			ChangeSourceWindow(CIW_TPL_SUPPORT, IW_ORDERS, (History..Occurrence), pCallback)
      	END
      	
      	IF UWPMenuOpen THEN
      		ExecuteProcess(BTN_MENU, trtClick)
      	END
     type : 458752
   -
     name : DisplayPreviousPlane
     procedure_id : 1760075514859705138
     type_code : 14
     code : |1-
      PROCEDURE DisplayPreviousPlane()
      
      	DisplayPlane(History[History..Occurrence], False)
     type : 458752
